<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SmartOffice.Views.MainPage"
             NavigationPage.HasBackButton="False">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Уведомления" Clicked="OnNotify"></Button>
            <Button Text="Профиль" Clicked="OnProfile"></Button>
            <Button Text="QR вход" Clicked="OnQRauth"></Button>
            <Button Text="Помещение" Clicked="OnLocation"></Button>
            <Button Text="Бронь" Clicked="OnRent"></Button>
            <Button Text="Помощь" Clicked="OnHelp"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>